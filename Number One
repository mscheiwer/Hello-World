function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  fill(255,255,255)
  triangle(230,100,250,45,200,100)
  circle(250,50,25)
  rect(200,100,25,175)
  rect(195,225,35,10)
  rect(195,250,35,10)
  rect(195,275,35,10)
  arc(150,285,50,50,QUARTER_PI,HALF_PI+PI)
  rect(125,285,175,100)
  fill(0,0,0)
  ellipse(225,375,175,12);
  line(203,280,203,95);
  line(206,280,206,90);
  line(210,280,210,85);
  line(214,280,214,80);
  line(218,280,218,75);
  line(222,280,222,70);
  circle(200,95,8);
  circle(205,90,8);
  circle(210,85,8);
  circle(215,80,8);
  circle(220,75,8);
  circle(225,70,8);
  arc(300,300,50,50,55,HALF_PI+HALF_PI);
  arc(300,350,50,50,QUARTER_PI=PI,HALF_PI);
  rect(225,300,75,50)
  text('Number One',325,390)
}
